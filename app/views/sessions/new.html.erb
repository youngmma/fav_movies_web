<!-- Get sign in input from the user -->

<div class="page-header">
<h1>Sign In</h1>
</div>

<%= form_tag "/sessions" do %>
	<p>
	  <%= label_tag :email, "Email: " %>
	  <%= text_field_tag :email %>
	</p><p>
	  <%= label_tag :password, "Password: " %>
	  <%= password_field_tag :password  %>
	</p>
  <%= submit_tag "Login" %>
<% end %>